e2856e2d31b4"],

Cell[BoxData[
 RowBox[{"\[Gamma]", "\[Equal]", 
  FractionBox["1", 
   SqrtBox[
    RowBox[{"1", "-", 
     FractionBox[
      SuperscriptBox["v", "2"], 
      SuperscriptBox["c", "2"]]}]]]}]], "Input",
 CellChangeTimes->{{3.900000000000005*^9, 
  3.900000000000005*^9}},ExpressionUUID->"af249f6b-7000-2640-b446-\
1bfa4a7982d4"]
}, Open  ]],

